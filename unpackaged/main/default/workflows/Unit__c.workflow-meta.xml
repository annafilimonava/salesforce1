    <rules>
        <fullName>Unit Ownership CH</fullName>
        <actions>
            <name>Unit_Ownership_CH</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>AND(Owner:Queue.QueueName &lt;&gt; &quot;POS Support CH&quot;,  TEXT(Location__c) = &quot;Switzerland&quot;)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
