    <rules>
        <fullName>Fallback Online</fullName>
        <actions>
            <name>Fallback_Online</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>POS_Solution__c.Fallback__c</field>
            <operation>equals</operation>
            <value>Fallback Online</value>
        </criteriaItems>
        <criteriaItems>
            <field>POS_Solution__c.Owning_Country__c</field>
            <operation>equals</operation>
            <value>Italy</value>
        </criteriaItems>
        <criteriaItems>
            <field>POS_Solution__c.Send_Fallback_Online_Email__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <description>Sends email to Contact on POS Solution when Fallback Online select on POS Solution in Italy.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
