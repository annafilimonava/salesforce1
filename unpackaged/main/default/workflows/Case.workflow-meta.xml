            <name>Case_Due</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Case.Due_Date__c</field>
            <operation>equals</operation>
            <value>TODAY</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.Status</field>
            <operation>notEqual</operation>
            <value>Back to AM,Forwarded to POS,Forwarded to CS,Forwarded to Finance,Closed</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.Owner_Role__c</field>
            <operation>equals</operation>
            <value>UK</value>
        </criteriaItems>
