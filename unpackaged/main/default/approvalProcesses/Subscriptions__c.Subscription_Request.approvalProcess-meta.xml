<?xml version="1.0" encoding="UTF-8"?>
<ApprovalProcess xmlns="http://soap.sforce.com/2006/04/metadata">
    <active>true</active>
    <allowRecall>false</allowRecall>
    <allowedSubmitters>
        <type>allInternalUsers</type>
    </allowedSubmitters>
    <approvalPageFields>
        <field>Name</field>
        <field>Account_ID__c</field>
        <field>CreatedBy</field>
        <field>Report_Name__c</field>
        <field>Recurrence__c</field>
        <field>Approval_Status__c</field>
        <field>Recipient_Email__c</field>
        <field>CurrencyIsoCode</field>
    </approvalPageFields>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <assignedApprover>
            <approver>
                <name>chirag.lalitcumar@planetpayment.com</name>
                <type>user</type>
            </approver>
            <approver>
                <name>intelligence@planetpayment.com</name>
                <type>user</type>
            </approver>
            <approver>
                <name>tim.moyce@planetpayment.com</name>
                <type>user</type>
            </approver>
            <approver>
                <name>vinod.paul@planetpayment.com</name>
                <type>user</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <label>Sent for Approval</label>
        <name>Sent_for_Approval</name>
    </approvalStep>
    <emailTemplate>unfiled$public/BI_Subscription_Request</emailTemplate>
    <enableMobileDeviceAccess>false</enableMobileDeviceAccess>
    <finalApprovalActions>
        <action>
            <name>Approval</name>
            <type>FieldUpdate</type>
        </action>
    </finalApprovalActions>
    <finalApprovalRecordLock>false</finalApprovalRecordLock>
    <finalRejectionActions>
        <action>
            <name>Rejection</name>
            <type>FieldUpdate</type>
        </action>
    </finalRejectionActions>
    <finalRejectionRecordLock>false</finalRejectionRecordLock>
    <initialSubmissionActions>
        <action>
            <name>Change_Status</name>
            <type>FieldUpdate</type>
        </action>
    </initialSubmissionActions>
    <label>Subscription Request</label>
    <processOrder>1</processOrder>
    <recordEditability>AdminOnly</recordEditability>
    <showApprovalHistory>true</showApprovalHistory>
</ApprovalProcess>
