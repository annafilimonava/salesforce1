<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Head_Office_Stock</fullName>
    <active>false</active>
    <errorConditionFormula>AND(NOT(ISBLANK(Head_Office__c)),  TEXT(Status__c) &lt;&gt; &quot;Stock&quot;)</errorConditionFormula>
    <errorDisplayField>Head_Office__c</errorDisplayField>
    <errorMessage>You may only complete the Head Office field if the Unit Status is Stock.</errorMessage>
</ValidationRule>
