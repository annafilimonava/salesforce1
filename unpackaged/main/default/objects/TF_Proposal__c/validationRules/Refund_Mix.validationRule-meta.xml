<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Refund_Mix</fullName>
    <active>false</active>
    <errorConditionFormula>Refund_Mix_Cash__c + Refund_Mix_Credit_Card__c + Refund_Mix_Other__c &gt; 1</errorConditionFormula>
    <errorDisplayField>Refund_Mix_Other__c</errorDisplayField>
    <errorMessage>The Total Refund Mix cannot be greater than 100.</errorMessage>
</ValidationRule>
