<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Focus_Point_Name</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISCHANGED(Name),$Profile.Name &lt;&gt; &quot;System Administrator&quot;)</errorConditionFormula>
    <errorDisplayField>Name</errorDisplayField>
    <errorMessage>You may not change the name of a Focus Point.</errorMessage>
</ValidationRule>
