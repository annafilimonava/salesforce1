<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>GBTF_Blank_Only</fullName>
    <active>true</active>
    <errorConditionFormula>AND(TEXT(Type__c) &lt;&gt; &quot;GB Tax Free&quot;,  ISBLANK(MID__c))</errorConditionFormula>
    <errorDisplayField>MID__c</errorDisplayField>
    <errorMessage>You must enter a Merchant ID.</errorMessage>
</ValidationRule>
